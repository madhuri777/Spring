/**
 * Hello world!
 *
 */
public class App 
{
    public static void main( String[] args )
    {
    	ApplicationContext context = new ClassPathXmlApplicationContext("Beans.xml");
    	HelloWord wd=(HelloWord) context.getBean("helloWord");
    }
}
