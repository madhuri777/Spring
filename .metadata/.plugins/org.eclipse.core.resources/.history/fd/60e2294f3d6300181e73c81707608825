package com.bridgeit.firstprogram;
import org.springframework.context.ApplicationContext;
import org.springframework.context.support.ClassPathXmlApplicationContext;
/**
 * Hello world!
 *
 */
public class App 
{
    public static void main( String[] args )
    {
    	@SuppressWarnings("resource")
		ApplicationContext context = new ClassPathXmlApplicationContext("/com/bridgeit/firstprogram/spring.xml");
    	HelloWord wd=(HelloWord) context.getBean("helloWorld");
    	//wd.setMessage("This is object one");
    	wd.getMessage();
    }
}
